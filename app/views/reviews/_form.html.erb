<%= simple_form_for @review do |f| %>

  <%= render "shared/errors", object: @review %>

  <%= f.input :title %>
  <%= f.input :restaurant %>
  <%= f.input :address %>
  <%= f.input :body %>
  <%= f.input :cuisine %>
  <%= f.input :price %>
  <%= f.input :score %>
  <%= f.input :ambiance %>
  <br>
  <%= f.button :submit, class: "orange-button" %>

<% end %>
